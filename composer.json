{
    "name": "johnroyer/url-normalizer",
    "description": "Syntax based normalization of URL's",
    "license": "MIT",
    "authors": [
        {
            "name": "Zero Huang",
            "email": "john.royer@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6.0",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Url\\" : "src/Url"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    }
}
